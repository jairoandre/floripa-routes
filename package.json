{
  "name": "floripa-routes",
  "version": "1.0.0",
  "description": "Find routes in Floripa!",
  "main": "webpack.config.js",
  "scripts": {
    "android": "cordova run android",
    "ios": "cordova run ios",
    "prepare": "node config && webpack && cordova prepare",
    "build": "node config && webpack && cordova build",
    "start": "node config && webpack-dev-server --progress --hot --port 3030 --colors",
    "phonegap": "node config && webpack --progress --colors && phonegap serve",
    "test": "mocha --colors --compilers js:babel-register --recursive ./test/**/*.js"
  },
  "dependencies": {
    "material-design-icons": "^2.1.1",
    "material-ui": "^0.14.4",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-gmaps": "^1.4.2",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-es2015-classes": "^6.5.2",
    "babel-plugin-transform-es2015-object-super": "^6.5.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
